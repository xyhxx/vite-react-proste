{
  "name": "vite-react-proste",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "dev:e2e": "cross-env IS_E2E=true vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "cypress:run": "cypress run -b chrome",
    "cypress": "cypress open",
    "lint:style": "stylelint packages/app/src *.*.css",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:style-fix": "pnpm run lint:style --fix",
    "lint:js-fix": "pnpm run lint:script --fix",
    "prettier": "prettier -c --write **/*",
    "prepare": "husky install",
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "commit": "cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "types": {
        "feat": {
          "description": "A new feature",
          "title": "Features"
        },
        "fix": {
          "description": "A bug fix",
          "title": "Bug Fixes"
        },
        "update": {
          "description": "Modify src files",
          "title": "Updates"
        },
        "chore": {
          "description": "Other changes that don't modify src or test files",
          "title": "Chores"
        },
        "test": {
          "description": "Adding missing tests or correcting existing tests",
          "title": "Tests"
        },
        "style": {
          "description": "Changes that do not affect the meaning of the code (white-space, formatting, missing semi-colons, etc)",
          "title": "Styles"
        },
        "refactor": {
          "description": "A code change that neither fixes a bug nor adds a feature",
          "title": "Code Refactoring"
        },
        "perf": {
          "description": "A code change that improves performance",
          "title": "Performance Improvements"
        },
        "revert": {
          "description": "Reverts a previous commit",
          "title": "Reverts"
        },
        "merge": {
          "description": "Merge branch",
          "title": "Merges"
        }
      }
    }
  },
  "lint-staged": {
    "**/*/*.css": [
      "stylelint",
      "prettier --write"
    ],
    "**/*/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">= 5%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.2",
    "axios": "^1.3.5",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.3",
    "react-router-dom": "^6.10.0",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@nabla/vite-plugin-eslint": "^1.5.0",
    "@tanstack/react-query-devtools": "^4.29.2",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "browserslist-to-esbuild": "^1.2.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.9.0",
    "cz-conventional-changelog": "^3.3.0",
    "editorconfig": "^1.0.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-proste": "^5.1.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "postcss-nesting": "^11.2.2",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.7",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "standard-version": "^9.5.0",
    "stylelint": "^15.4.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^32.0.0",
    "swc-plugin-react-remove-properties": "^0.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1",
    "web-vitals": "^3.3.1"
  }
}
{
  "name": "vite-react-proste",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "dev:e2e": "cross-env IS_E2E=true vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "cypress:run": "cypress run -b chrome",
    "cypress": "cypress open",
    "lint:style": "stylelint packages/app/src *.*.css",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:style-fix": "pnpm run lint:style --fix",
    "lint:js-fix": "pnpm run lint:js --fix",
    "prepare": "husky install",
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "commit": "cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "types": {
        "feat": {
          "description": "A new feature",
          "title": "Features"
        },
        "fix": {
          "description": "A bug fix",
          "title": "Bug Fixes"
        },
        "update": {
          "description": "Modify src files",
          "title": "Updates"
        },
        "chore": {
          "description": "Other changes that don't modify src or test files",
          "title": "Chores"
        },
        "test": {
          "description": "Adding missing tests or correcting existing tests",
          "title": "Tests"
        },
        "style": {
          "description": "Changes that do not affect the meaning of the code (white-space, formatting, missing semi-colons, etc)",
          "title": "Styles"
        },
        "refactor": {
          "description": "A code change that neither fixes a bug nor adds a feature",
          "title": "Code Refactoring"
        },
        "perf": {
          "description": "A code change that improves performance",
          "title": "Performance Improvements"
        },
        "revert": {
          "description": "Reverts a previous commit",
          "title": "Reverts"
        },
        "merge": {
          "description": "Merge branch",
          "title": "Merges"
        }
      }
    }
  },
  "lint-staged": {
    "**/*/*.css": [
      "stylelint"
    ],
    "**/*/*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "browserslist": {
    "production": [
      ">= 5%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.15",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-router-dom": "^6.13.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@nabla/vite-plugin-eslint": "^1.5.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "browserslist-to-esbuild": "^1.2.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.15.0",
    "cz-conventional-changelog": "^3.3.0",
    "editorconfig": "^1.0.2",
    "eslint": "^8.43.0",
    "eslint-config-proste": "^7.6.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-newlines": "^1.3.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-xyhxx": "^0.1.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "postcss-nesting": "^11.3.0",
    "postcss-preset-env": "^8.5.1",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "standard-version": "^9.5.0",
    "stylelint": "^15.9.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-standard": "^33.0.0",
    "swc-plugin-react-remove-properties": "^0.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  }
}
